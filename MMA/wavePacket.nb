Notebook[{

Cell[CellGroupData[{
Cell["Wave Packet", "Section",
 CellChangeTimes->{{3.61131869103267*^9, 3.6113186968308477`*^9}}],

Cell[CellGroupData[{

Cell["Non Relativistic Gaussian", "Subsection",
 CellChangeTimes->{{3.611318992901647*^9, 3.611318993996089*^9}, {
  3.6113240797033777`*^9, 3.611324083341556*^9}}],

Cell["\<\
The Gaussian wave packet for free Schrodinger equaiton is\
\>", "Text",
 CellChangeTimes->{{3.611318996231778*^9, 3.611319030815277*^9}}],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[Psi]", "(", 
    RowBox[{"x", ",", "t"}], ")"}], "=", 
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      FractionBox[
       SubscriptBox["\[Sigma]", "0"], 
       SqrtBox[
        RowBox[{"2", "\[Pi]"}]]], ")"}], 
     RowBox[{"1", "/", "2"}]], 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox[
        SubscriptBox["\[Sigma]", "0"], "2"], "+", 
       FractionBox[
        RowBox[{"i", " ", "t"}], "2"]}], ")"}], 
     RowBox[{
      RowBox[{"-", "1"}], "/", "2"}]], 
    RowBox[{"exp", "(", 
     RowBox[{
      RowBox[{"-", 
       SuperscriptBox[
        SubscriptBox["\[Sigma]", "0"], "2"]}], 
      SuperscriptBox[
       SubscriptBox["p", "0"], "2"]}], ")"}], 
    RowBox[{"exp", "(", 
     RowBox[{"-", 
      FractionBox[
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"x", "-", 
          RowBox[{"2", "i", " ", 
           SuperscriptBox[
            SubscriptBox["\[Sigma]", "0"], "2"], 
           SubscriptBox["p", "0"]}]}], ")"}], "2"], 
       RowBox[{"4", 
        RowBox[{"(", 
         RowBox[{
          SuperscriptBox[
           SubscriptBox["\[Sigma]", "0"], "2"], "+", 
          RowBox[{"i", " ", 
           RowBox[{"t", "/", "2"}]}]}], ")"}]}]]}], ")"}]}]}], 
  TraditionalForm]], "InlineMath"]], "Text",
 CellChangeTimes->{3.6113190608600473`*^9}],

Cell["\<\
To check the width of this wave packet, it\[CloseCurlyQuote]s better to write \
down the probability distribution,\
\>", "Text",
 CellChangeTimes->{{3.611319065016334*^9, 3.611319089921709*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[Rho]", "(", 
    RowBox[{"x", ",", "t"}], ")"}], "=", 
   RowBox[{
    RowBox[{"|", 
     RowBox[{"\[Psi]", "(", 
      RowBox[{"x", ",", "t"}], ")"}], 
     SuperscriptBox["|", "2"]}], "=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        SqrtBox[
         RowBox[{"2", "\[Pi]"}]], 
        RowBox[{"\[Sigma]", "(", "t", ")"}]}], ")"}], 
      RowBox[{"-", "1"}]], 
     RowBox[{"exp", "(", 
      RowBox[{"-", 
       FractionBox[
        SuperscriptBox[Cell[TextData[{
          Cell[BoxData[
           FormBox[
            RowBox[{"(", 
             RowBox[{"x", "-", 
              RowBox[{
               SubscriptBox["p", "0"], "t"}]}]}], TraditionalForm]], 
           "InlineMath"],
          ")"
         }], "InlineMath"], "2"], 
        RowBox[{"2", 
         SuperscriptBox[
          RowBox[{"\[Sigma]", "(", "t", ")"}], "2"]}]]}], ")"}]}]}]}], 
  TraditionalForm]], "Input"],

Cell["with a width", "Text",
 CellChangeTimes->{{3.6113191028046*^9, 3.611319110009091*^9}}],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[Sigma]", "(", "t", ")"}], "=", 
   SuperscriptBox[
    RowBox[{
     SubscriptBox["\[Sigma]", "0"], "(", 
     RowBox[{"1", "+", 
      FractionBox[
       SuperscriptBox["t", "2"], 
       RowBox[{"4", 
        SuperscriptBox[
         SubscriptBox["\[Sigma]", "0"], "4"]}]]}], ")"}], 
    RowBox[{"1", "/", "2"}]]}], TraditionalForm]]]], "Text",
 CellChangeTimes->{3.611319123038875*^9}],

Cell[TextData[{
 "Here we use ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[HBar]", "=", 
    RowBox[{"m", "=", "1"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ". To restore the units, we have"
}], "Text",
 CellChangeTimes->{{3.611319414449688*^9, 3.6113194175843687`*^9}, {
  3.611319493052084*^9, 3.6113195327755938`*^9}}],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Sigma]", "(", "t", ")"}], "=", 
    SuperscriptBox[
     RowBox[{
      SubscriptBox["\[Sigma]", "0"], "(", 
      RowBox[{"1", "+", 
       RowBox[{
        FractionBox[
         SuperscriptBox["t", "2"], 
         RowBox[{"4", 
          SuperscriptBox[
           SubscriptBox["\[Sigma]", "0"], "4"]}]], 
        SuperscriptBox[
         RowBox[{"(", 
          FractionBox["\[HBar]", "m"], ")"}], "2"]}]}], ")"}], 
     RowBox[{"1", "/", "2"}]]}], TraditionalForm]]],
 "."
}], "Text",
 CellChangeTimes->{{3.611319537362256*^9, 3.6113195572237883`*^9}}],

Cell[CellGroupData[{

Cell["For the case of neutrinos:", "Subsubsection",
 CellChangeTimes->{{3.6113198427995977`*^9, 3.6113198616777277`*^9}}],

Cell[TextData[{
 "Neutrino mass: ~ ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", 
    RowBox[{"eV", "/", 
     SuperscriptBox["c", "2"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " ~ ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1.78", "\[Cross]", 
    SuperscriptBox["10", 
     RowBox[{"-", "36"}]]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "kg, we have ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[HBar]", "=", 
    RowBox[{"6.58", "\[Cross]", 
     SuperscriptBox["10", 
      RowBox[{"-", "16"}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "eV s, so \[HBar]/m is"
}], "Text",
 CellChangeTimes->{{3.611321222458282*^9, 3.6113212263866568`*^9}, {
  3.6113212698065166`*^9, 3.611321279101387*^9}, {3.611321643852334*^9, 
  3.611321644251396*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"hbarOm", "=", 
  RowBox[{"6.58", " ", 
   SuperscriptBox["10", 
    RowBox[{"-", "16"}]], " ", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"3", " ", 
      RowBox[{"10", "^", "8"}]}], ")"}], "^", "2"}]}]}]], "Input",
 CellChangeTimes->{{3.611321243130068*^9, 3.6113212590795803`*^9}}],

Cell[BoxData["59.220000000000006`"], "Output",
 CellChangeTimes->{3.611321262444756*^9}]
}, Open  ]],

Cell["Now the expansion is", "Text",
 CellChangeTimes->{{3.611321506732403*^9, 3.611321511890606*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"sigma", "[", 
   RowBox[{"t_", ",", "sigma0_"}], "]"}], ":=", 
  RowBox[{"sigma0", " ", 
   SqrtBox[
    RowBox[{"1", "+", 
     RowBox[{
      RowBox[{
       RowBox[{"t", "^", "2"}], "/", 
       RowBox[{"(", 
        RowBox[{"4", " ", 
         RowBox[{"sigma0", "^", "4"}]}], ")"}]}], "hbarOm"}]}]]}]}]], "Input",\

 CellChangeTimes->{{3.611321521739431*^9, 3.61132154840399*^9}, 
   3.611321633717987*^9, {3.6113216643976717`*^9, 3.6113216943528748`*^9}}],

Cell["\<\
If we use the initial condition sigma0=10^{-10}cm=10^{-12}m, we have\
\>", "Text",
 CellChangeTimes->{{3.611321705139955*^9, 3.611321748336974*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"sigmaI1", "[", "t_", "]"}], "=", 
  RowBox[{"sigma", "[", 
   RowBox[{"t", ",", 
    RowBox[{"10", "^", 
     RowBox[{"{", 
      RowBox[{"-", "12"}], "}"}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.611321751027953*^9, 3.611321826289814*^9}, {
  3.6113219551005707`*^9, 3.611321956609728*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  FractionBox[
   SqrtBox[
    RowBox[{"1", "+", 
     RowBox[{"1.4805000000000002`*^49", " ", 
      SuperscriptBox["t", "2"]}]}]], "1000000000000"], "}"}]], "Output",
 CellChangeTimes->{{3.611321816390505*^9, 3.611321827533617*^9}, {
  3.611321957503643*^9, 3.61132196018123*^9}}]
}, Open  ]],

Cell[TextData[{
 "For not very small t, this is ",
 Cell[BoxData[
  RowBox[{"3.8477266015141978`*^12", "t"}]]],
 " , which grows linearly."
}], "Text",
 CellChangeTimes->{{3.611322005460946*^9, 3.6113220251613827`*^9}}],

